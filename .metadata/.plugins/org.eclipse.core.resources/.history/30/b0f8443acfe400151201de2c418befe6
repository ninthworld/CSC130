package org.ninthworld.binarytrees;

public class RBNode extends BSNode {

	private int value;
	private RBColor color;
	private RBNode left, right;
	
	public RBNode(int value) {
		super(value);
		this.value = value;
		this.color = RBColor.RED;
		this.left = null;
		this.right = null;
	}

}

enum RBColor {
	RED, BLACK
};