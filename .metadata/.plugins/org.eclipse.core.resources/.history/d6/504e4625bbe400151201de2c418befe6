package org.ninthworld.trees;

public class BSNode {
	
	private int value;
	private BSNode left, right;
	
	public BSNode(int value){
		this.value = value;
		this.left = null;
		this.right = null;
	}
	
	public BSNode getLeft(){
		return left;
	}
	
	public void setLeft(BSNode node){
		left = node;
	}
	
	public BSNode getRight(){
		return right;
	}
	
	public void setRight(BSNode node){
		right = node;
	}
	
	public int getValue(){
		return value;
	}
	
	public void setValue(int value){
		this.value = value;
	}
}
