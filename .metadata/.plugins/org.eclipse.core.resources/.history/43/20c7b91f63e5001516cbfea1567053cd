package org.ninthworld.binarytrees;

import java.awt.Color;

public class DisplayGraphData {

	private double x, y;
	private Color color;
	
	public DisplayGraphData(double x, double y) {
		this(x, y, Color.BLACK);
	}
	
	public DisplayGraphData(double x, double y, Color color) {
		this.x = x;
		this.y = y;
		this.color = color;
	}
}
